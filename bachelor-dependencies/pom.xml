<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>cn.org.bachelor</groupId>
    <artifactId>bachelor-dependencies</artifactId>
    <version>2.1-SNAPSHOT</version>
    <name>bachelor-dependencies</name>
    <description>bachelor-dependencies</description>
    <packaging>pom</packaging>
    <developers>
        <developer>
            <name>bachelor</name>
            <email>liuzhuo84@hotmail.com</email>
            <organization>bachelor</organization>
            <organizationUrl>http://www.bachelor.org.cn</organizationUrl>
        </developer>
    </developers>
    <distributionManagement>
        <repository>
            <id>central_maven_release</id>
            <name>releases</name>
            <url>https://oss.sonatype.org/service/local/staging/deploy/maven2</url>
        </repository>
        <snapshotRepository>
            <id>central_maven_snapshot</id>
            <name>snapshots</name>
            <url>https://oss.sonatype.org/content/repositories/snapshots</url>
            <layout>default</layout>
        </snapshotRepository>
    </distributionManagement>
    <properties>
        <java.version>1.8</java.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <spring-boot.version>2.3.2.RELEASE</spring-boot.version>
        <spring-cloud.version>Hoxton.SR10</spring-cloud.version>
        <swagger2.version>2.9.2</swagger2.version>
        <knife4j.version>2.0.8</knife4j.version>
        <github-pagehelper.version>1.2.13</github-pagehelper.version>
        <tk-mybatis.version>2.1.5</tk-mybatis.version>
        <tk-mapperbase.version>1.1.5</tk-mapperbase.version>
        <tk-mappergenerator.version>1.1.5</tk-mappergenerator.version>
        <spring-boot-admin.version>2.2.3</spring-boot-admin.version>
        <mybatis.version>2.1.3</mybatis.version>
        <fastjson.version>1.2.68</fastjson.version>
        <spring-json.version>1.0.2</spring-json.version>
        <spring-security-jwt.version>1.1.1.RELEASE</spring-security-jwt.version>
        <mybatis-generator-core.version>1.4.0</mybatis-generator-core.version>
        <bucket4j-core.version>4.9.0</bucket4j-core.version>
        <versions-maven-plugin.version>2.7</versions-maven-plugin.version>
        <spring-boot-plugin.version>2.3.1.RELEASE</spring-boot-plugin.version>
        <jettison.version>1.2</jettison.version>
        <sbcp.version>2.4.1</sbcp.version>
        <feign.version>1.4.4.RELEASE</feign.version>
        <bachelor-fw.version>2.1-SNAPSHOT</bachelor-fw.version>
        <bachelor-iam.version>2.1-SNAPSHOT</bachelor-iam.version>
        <bachelor-oauth2.version>2.0-SNAPSHOT</bachelor-oauth2.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.springframework.cloud</groupId>
                <artifactId>spring-cloud-dependencies</artifactId>
                <version>${spring-cloud.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-dependencies</artifactId>
                <version>${spring-boot.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <dependency>
                <groupId>io.springfox</groupId>
                <artifactId>springfox-swagger-ui</artifactId>
                <version>${swagger2.version}</version>
            </dependency>
            <dependency>
                <groupId>io.springfox</groupId>
                <artifactId>springfox-swagger2</artifactId>
                <version>${swagger2.version}</version>
            </dependency>
            <dependency>
                <groupId>com.github.xiaoymin</groupId>
                <artifactId>knife4j-micro-spring-boot-starter</artifactId>
                <version>${knife4j.version}</version>
            </dependency>
            <dependency>
                <groupId>com.github.pagehelper</groupId>
                <artifactId>pagehelper-spring-boot-starter</artifactId>
                <version>${github-pagehelper.version}</version>
            </dependency>
            <dependency>
                <groupId>tk.mybatis</groupId>
                <artifactId>mapper-spring-boot-starter</artifactId>
                <version>${tk-mybatis.version}</version>
            </dependency>
            <dependency>
                <groupId>tk.mybatis</groupId>
