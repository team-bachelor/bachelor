            </dependency>
            <dependency>
                <groupId>cn.org.bachelor</groupId>
                <artifactId>bachelor-iam-service</artifactId>
                <version>${bachelor-iam.version}</version>
            </dependency>
            <dependency>
                <groupId>cn.org.bachelor</groupId>
